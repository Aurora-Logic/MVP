{
  "compilerOptions": {
    "checkJs": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": false,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": ["assets/js/**/*.js", "assets/js/globals.d.ts"],
  "exclude": ["node_modules", "dist", "test-results", "assets/js/vendor"]
}
